<query id="getDonotsend" action="select">
    <tables>
        <table name="ggmailing_donotsends" />
    </tables>
    <columns>
		<column name="*" />
    </columns>  
	<conditions>
		<condition operation="equal" column="ggmailing_nickname" var="ggmailing_nickname" />
		<condition operation="equal" column="ggmailing_email" var="ggmailing_email" pipe="and" />
		<condition operation="equal" column="ggmailing_member_regdate" var="ggmailing_member_regdate" pipe="and" />
	</conditions>
</query>
